{
  "name": "@imageengine/vue3",
  "version": "0.1.0",
  "author": "Siva Sankar Reddy Bogala",
  "private": false,
  "repository": "https://github.com/imgeng/imageengine-vue3",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.js"
    }
  },
  "scripts": {
    "build": "run-p type-check test:unit:build build-only",
    "build-only": "vite build",
    "dev": "vite",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "type-check": "vue-tsc --noEmit --composite false",
    "test:unit:build": "cross-env ENV=\"TEST\" vitest --environment jsdom --root src/ --run --silent",
    "test:unit": "cross-env ENV=\"TEST\" vitest --environment jsdom --root src/ --silent",
    "prepare": "husky install"
  },
  "dependencies": {
    "@imageengine/imageengine-helpers": "^0.2.0",
    "vue": "3.4.14"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.6.1",
    "@vitejs/plugin-vue": "4.6.2",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "2.4.3",
    "cross-env": "^7.0.3",
    "eslint": "8.56.0",
    "eslint-plugin-vue": "9.20.1",
    "husky": "^8.0.2",
    "jsdom": "^22.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "tslib": "2.6.2",
    "vite": "4.5.1",
    "vite-plugin-dts": "3.7.1",
    "vitest": "^0.34.0",
    "vue-tsc": "1.8.27"
  }
}
